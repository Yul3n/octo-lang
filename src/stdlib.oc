-- Booleans

type bool =
  True | False

if True x _ = x
if False _ y = y

iszero 0 = True
iszero n = False

-- Combinators

id x =
  x

cons x _ =
  x

compose f g x =
  f (g x)

flip f g x =
  g (f x)

-- Lists

map f l =
  case l of
    [] -> []
    hd :: tl -> (f hd) :: (map f tl)

isnil [] = True
isnil l  = False
